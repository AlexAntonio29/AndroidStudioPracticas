<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/itemObjectIngreso"
    android:orientation="horizontal"
    android:layout_marginTop="40dp"
    android:layout_marginLeft="20dp"
    >



    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:stretchColumns="1">

        <TableRow>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/nameIngreso"
                android:text="NameIngreso"



                />
            <TextView
            android:layout_height="wrap_content"
                android:layout_width="wrap_content"/>


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/cantidadProducto"
                android:text="Cantidad"
                android:layout_marginLeft="20dp"

                />
            <TextView
                android:layout_marginLeft="20dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/simbolo1"
                android:text="$"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/precioUnitario"
                android:text="PrecioUnitario"


                />
            <TextView
                android:layout_marginLeft="20dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/simbolo2"
                android:text="$"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/precioTotal"
                android:text="Total"
                android:layout_marginRight="15dp"



                />

        </TableRow>


    </TableLayout>


</LinearLayout>